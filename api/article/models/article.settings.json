{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "date": {
      "type": "date"
    },
    "url": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "body": {
      "type": "richtext",
      "required": true
    }
  }
}
